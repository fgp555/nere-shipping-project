  "consignee": "ABC Corporation",
  "marks": "Mark1",
  "qtyPkgs": 10,
  "remarks": "No remarks",
  "pallet": "Pallet1",
  "legend": "Special instruction",
  "images": [
    {
      "path": "uploads/favicon.png"
